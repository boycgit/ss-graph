{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": ".+\\.(ts|js)(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "idInHeadings": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}